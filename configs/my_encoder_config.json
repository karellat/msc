{
  "model" : "3d_myencoder",
  "model_args" : {
    "input_shape"  : [32, 32, 32, 1],
    "init_filters" : 256,
  },
  "dataset_path" : "default",
  "filter_first_scan" : false,
  "dataset" : "encoder",
  "dataset_args" : {
    "normalize" : true,
    "box_size" : 32,
    "downscale_ratio" : null,
    "boxes_per_img" : 4
  },

  "callbacks" : ["board", "save", "lr_poly", "early_stop"],
  "log_root" : "./logs",
  "log_name" : "my_encoder",
  "epochs" : 100,
  "batch_size" : 8,
  "optimizer" : "adam",
  "loss" : "mse",
  "metrics" : ["mse"]

}
