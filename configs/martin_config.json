{
  "model" : "3d_martin",
  "model_args" : {
    "input_shape"  : [97, 115, 97, 1],
                "init_filters":256,
                 "conv_layers":5,
                 "conv_kernel":2,
                 "conv_activation":"relu",
                 "conv_stride":2,
                 "conv_to_fc":null
  },
  "dataset_path" : "default",
  "data_csv_path" : "default",
  "train_filter_first_screen" : true,
  "valid_filter_first_screen" : true,
  "dataset" : "3d",
  "dataset_args" : {
    "normalize" : true,
    "downscale_ratio" : 2
  },

  "callbacks" : ["board", "save"],
  "log_root" : "./logs/martin",
  "log_name" : "L5-F256-relu-None",
  "epochs" : 100,
  "batch_size" : 8,
  "optimizer" : "adam",
  "loss" : "categorical_crossentropy",
  "metrics" : ["accuracy", "AUC"]

}
